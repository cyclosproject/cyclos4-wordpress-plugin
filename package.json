{
	"name": "cyclos",
	"version": "1.0.0",
	"description": "Development for the Cyclos4 WordPress plugin.",
	"author": "The Cyclos team <info@cyclos.org> (https://www.cyclos.org/)",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"cyclos"
	],
	"homepage": "https://wordpress.org/plugins/cyclos/",
	"repository": {
		"type": "git",
		"url": "https://github.com/cyclosproject/cyclos4-wordpress-plugin.git"
	},
	"bugs": "https://wordpress.org/plugins/cyclos/",
	"private": true,
	"dependencies": {},
	"devDependencies": {
		"@wordpress/scripts": "12.3.0"
	},
	"scripts": {
		"build": "wp-scripts build js/src/admin.js js/src/cyclos_login.js --output-path=js/dist",
		"build:admin": "wp-scripts build js/src/admin.js --output-path=js/dist",
		"build:login": "wp-scripts build js/src/cyclos_login.js --output-path=js/dist",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format:js": "wp-scripts format-js ./js/src",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js ./js/src",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint:php": "composer wpcs:check",
		"start": "wp-scripts start",
		"start:login": "wp-scripts start js/src/cyclos_login.js --output-path=js/dist",
		"autoload:dev": "composer dump-autoload"
	}
}
